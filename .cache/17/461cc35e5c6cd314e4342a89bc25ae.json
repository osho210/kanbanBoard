"source":"Column.tsx","name":null,"original":{"line":109,"column":20},"generated":{"line":111,"column":20}},{"source":"Column.tsx","name":null,"original":{"line":109,"column":23},"generated":{"line":111,"column":23}},{"source":"Column.tsx","name":null,"original":{"line":109,"column":23},"generated":{"line":111,"column":48}},{"source":"Column.tsx","name":null,"original":{"line":109,"column":30},"generated":{"line":111,"column":49}},{"source":"Column.tsx","name":null,"original":{"line":109,"column":33},"generated":{"line":111,"column":52}},{"source":"Column.tsx","name":null,"original":{"line":109,"column":0},"generated":{"line":120,"column":1}}],"sources":{"Column.tsx":"import React, { useState } from 'react'\nimport styled from 'styled-components'\nimport * as color from './color'\nimport { Card } from './Card'\nimport { PlusIcon } from './icon'\nimport { InputForm as _InputForm } from './InputForm'\n\n// propsの受け取り\nexport function Column({ title, cards, }: {\n  title?: string\n  // cardsの要素の型宣言\n  cards: {\n    id: string\n    text?: string\n    // この配列何？\n  }[]\n}) {\n\n  //cardの要素数の表示\n  const totalCount = cards.length\n\n  // useStateは第1引数が変数、第2引数が変数上書きのための関数\n  const [text, setText] = useState('')\n  const [inputMode, setInputMode] = useState(false)\n  const toggleInput = () => setInputMode(v => !v)\n  const confirmInput = () => setInputMode(false)\n  const canselInput = () => setInputMode(false)\n\n  return (\n    // div要素\n    <Container>\n      <Header>\n        <CountBadge>{totalCount}</CountBadge>\n        <ColumnName>{title}</ColumnName>\n\n        <AddButton onClick={toggleInput} />\n      </Header>\n      {inputMode && (\n        <InputForm\n          // stateの値\n          value={text}\n          onChange={setText}\n          onConfirm={confirmInput}\n          onCancel={canselInput}\n        />\n      )}\n      <VerticalScroll>\n        {cards.map(({ id, text }) => (\n          // card内要素のprops渡し\n          <Card key={id} text={text} />\n        ))}\n      </VerticalScroll>\n    </Container>\n  )\n}\n\nconst Container = styled.div`\n  display: flex;\n  flex-flow: column;\n  width: 355px;\n  height: 100%;\n  border: solid 1px ${color.Silver};\n  border-radius: 6px;\n  background-color: ${color.LightSilver};\n\n  > :not(:last-child) {\n    flex-shrink: 0;\n  }\n`\n\nconst Header = styled.div`\n  display: flex;\n  justify-content: flex-start;\n  align-items: center;\n  padding: 8px;\n`\n\nconst CountBadge = styled.div`\n  margin-right: 8px;\n  border-radius: 20px;\n  padding: 2px 6px;\n  color: ${color.Black};\n  background-color: ${color.Silver};\n  font-size: 12px;\n  line-height: 1;\n`\n\nconst ColumnName = styled.div`\n  color: ${color.Black};\n  font-size: 14px;\n  font-weight: bold;\n`\n\nconst AddButton = styled.button.attrs({\n  type: 'button',\n  children: <PlusIcon />,\n})`\n  margin-left: auto;\n  color: ${color.Black};\n\n  :hover {\n    color: ${color.Blue};\n  }\n`\nconst InputForm = styled(_InputForm)`\npadding:8px\n`\n\nconst VerticalScroll = styled.div`\n  height: 100%;\n  padding: 8px;\n  overflow-y: auto;\n  flex: 1 1 auto;\n\n  > :not(:first-child) {\n    margin-top: 8px;\n  }\n`"},"lineCount":null}},"error":null,"hash":"a9b1023f12d4b5ab922f1e2c33e6519f","cacheData":{"env":{}}}